{
  "name": "raasta-monorepo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "dev:web": "turbo dev --filter=@raasta/web",
    "dev:studio": "turbo dev --filter=@raasta/studio",
    "build": "turbo build",
    "build:web": "turbo build --filter=@raasta/web",
    "build:studio": "turbo build --filter=@raasta/studio",
    "lint": "turbo lint",
    "clean": "turbo clean && rm -rf node_modules"
  },
  "devDependencies": {
    "turbo": "^2.3.3"
  },
  "packageManager": "pnpm@9.15.0"
}
